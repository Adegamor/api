{
  "parserOptions": {
    "ecmaVersion": 7,
    "ecmaFeatures": {
        "blockBindings": true,
        "experimentalObjectRestSpread": true
    }
  },
  "rules": {
    "comma-dangle": [1,"never"],
    "no-cond-assign": [2,"always"],
    "no-console": 2,
    "no-constant-condition": 2,
    "no-control-regex": 1,
    "no-debugger": 2,
    "no-dupe-args": 2,
    "no-dupe-keys": 2,
    "no-duplicate-case": 2,
    "no-empty-character-class": 1,
    "no-empty": 2,
    "no-ex-assign": 2,
    "no-extra-boolean-cast": 1,
    "no-extra-parens": [1,"functions"],
    "no-extra-semi": 2,
    "no-func-assign": 1,
    "no-inner-declarations": [1,"both"],
    "no-invalid-regexp": 2,
    "no-irregular-whitespace": 1,
    "no-negated-in-lhs": 1,
    "no-obj-calls": 1,
    "no-regex-spaces": 1,
    "no-sparse-arrays": 2,
    "no-unreachable": 2,
    "use-isnan": 1,
    "valid-typeof": 1,
    "no-unexpected-multiline": 1,
    "complexity": [1,10],
    "consistent-return": 1,
    "curly": [1,"all"],
    "default-case": 1,
    "eqeqeq": [1,"smart"],
    "no-alert": 2,
    "no-caller": 1,
    "no-div-regex": 1,
    "no-eq-null": 1,
    "no-eval": 2,
    "no-extra-bind": 1,
    "no-fallthrough": 1,
    "no-floating-decimal": 1,
    "no-implicit-coercion": [1,{"boolean":true,"number":true,"string":true}],
    "no-implied-eval": 2,
    "no-invalid-this": 1,
    "no-iterator": 1,
    "no-lone-blocks": 1,
    "no-loop-func": 2,
    "no-multi-str": 1,
    "no-new-func": 2,
    "no-new-wrappers": 1,
    "no-new": 1,
    "no-octal-escape": 2,
    "no-octal": 2,
    "no-param-reassign": 1,
    "no-proto": 1,
    "no-redeclare": 2,
    "no-return-assign": [1,"always"],
    "no-script-url": 1,
    "no-self-compare": 2,
    "no-sequences": 1,
    "no-throw-literal": 1,
    "no-useless-call": 1,
    "no-void": 2,
    "no-with": 2,
    "radix": 1,
    "vars-on-top": 1,
    "wrap-iife": [1,"any"],
    "yoda": [1,"always",{}],
    "strict": [1,"global"],
    "no-catch-shadow": 1,
    "no-delete-var": 2,
    "no-shadow-restricted-names": 1,
    "no-undef-init": 1,
    "no-undef": 2,
    "no-undefined": 1,
    "no-unused-vars": 1,
    "no-use-before-define": 1,
    "no-mixed-requires": 1,
    "no-new-require": 2,
    "no-path-concat": 2,
    "no-process-exit": 2,
    "no-sync": 1,
    "array-bracket-spacing": [1,"always",{}],
    "brace-style": [1,"1tbs",{}],
    "camelcase": [1,{"properties":"always"}],
    "comma-spacing": [1,{"after":true}],
    "eol-last": 1,
    "func-style": [1,"declaration"],
    "linebreak-style": [2,"unix"],
    "new-parens": 1,
    "no-mixed-spaces-and-tabs": [1,"smart-tabs"],
    "no-multiple-empty-lines": [1,{"max":3}],
    "no-nested-ternary": 1,
    "no-trailing-spaces": 1,
    "no-underscore-dangle": 1,
    "object-curly-spacing": [1,"always",{}],
    "operator-linebreak": [1,"before"],
    "semi-spacing": [1,{"after":true}],
    "sort-vars": 1,
    "space-return-throw-case": 1,
    "wrap-regex": 1
  }
}
