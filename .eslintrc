{
  "extends": "airbnb",
  "ecmaFeatures": {
    "blockBindings": true,
    "classes": true,
    "defaultParams": true,
    "destructuring": true,
    "forOf": false,
    "generators": true,
    "modules": false,
    "octalLiterals": true,
    "regexUFlag": true,
    "regexYFlag": true,
    "restParams": true,
    "spread": true,
    "superInFunctions": true,
    "templateStrings": true,
    "unicodeCodePointEscapes": true,
    "globalReturn": false
  },
  "rules": {
    "strict": [ 2, "global" ],
    "indent": [ 2, "tab" ],
    "max-len": [1, 160],
    "func-names": 0,
    "prefer-arrow-callback": 0,
    "space-after-keywords": [ 2, "always" ],
    "space-before-function-paren": [ 2, "never" ],
    "comma-dangle": [ 2, "never" ],
    "object-shorthand": [ 0, "never" ],
    "new-cap": 0,
    "comma-spacing": [ 1, { "before": false, "after": true }],
    "array-bracket-spacing": [ 1, "always"]
  }
}
